{
  "name": "country-info-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start:prod": "node ./dist/index.js",
    "start:dev:run": "nodemon ./dist/index.js",
    "start:dev": "npm run clean && npm run lint && npm run build && npm run start:dev:run",
    "clean": "rm -rf dist/*",
    "build": "tsup",
    "lint": "eslint ./src/",
    "migration:generate": "npx typeorm -- migration:generate src/database/migrations/AutoGeneratedMigration -d src/database/DataSource.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/database/DataSource.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/database/DataSource.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/country-code-lookup": "^0.0.0",
    "axios": "^1.8.4",
    "country-code-lookup": "^0.1.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "joi": "^17.13.3",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/axios": "^0.9.36",
    "@types/express": "^5.0.1",
    "@types/joi": "^17.2.2",
    "@types/node": "^22.14.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/__tests__/**",
      "!src/**/*.test.*"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
